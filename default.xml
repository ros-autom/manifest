<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com/"/>
	   
   <default revision="refs/heads/kinetic"
           remote="github"
           sync-c="true"
	   sync-j="4" />

  <project path="notes"  name="ros-autom/notes"  revision="greek" />
  
  <project path="autom-scripts" name="ros-autom/autom-scripts">
    <copyfile src="gazebo_farm.sh" dest="gazebo_farm.sh" />
    <copyfile src="gazebo_multi_robots.sh" dest="gazebo_multi_robots.sh" />
    <copyfile src="gazebo_warehouse.sh" dest="gazebo_warehouse.sh" />
    <copyfile src="run_teleop.sh" dest="run_teleop.sh" />
    <copyfile src="run_teleop_husky1.sh" dest="run_teleop_husky1.sh" />
    <copyfile src="run_teleop_husky2.sh" dest="run_teleop_husky2.sh" />
    <copyfile src="IGNOREFILE" dest=".gitignore" />
    <copyfile src="gazebo_multihusky_karto.sh" dest="gazebo_multihusky_karto.sh" />
  </project>
  
  <!--Android-->
  <!-- Shell scripts to wrap our custom Android ROS control app -->
  <project path="android/android_package" name="ros-autom/android_package" />
   <!--Android interface for ROS operation-->
  <project path="android/RobotCA" name="ros-autom/RobotCA" />
  <!--Android core-->
  <project path="android/android_core" name="ros-autom/android_core" />
  
   <!--ROS Autom forks and custom edits-->
  <project path="src/find-object"  name="ros-autom/find-object"  />
  <project path="src/husky"  name="ros-autom/husky"  />
  <project path="src/m-explore" name="ros-autom/m-explore" />
  <project path="src/navigation_2d" name="ros-autom/navigation_2d" />
  <project path="src/nre_simmultihusky" name="ros-autom/nre_simmultihusky"  />
  <project path="src/usb_cam" name="ros-autom/usb_cam"  />
  <project path="src/teb_local_planner" name="ros-autom/teb_local_planner" />
  <project path="src/eband_local_planner" name="ros-autom/eband_local_planner" />
  <project path="Arduino-ROS" name="ros-autom/Arduino-ROS"  />
  <project path="RaspberryPi-ROS" name="ros-autom/RaspberryPi-ROS"  />
  <project path="src/depthimage_to_laserscan" name="ros-autom/depthimage_to_laserscan" />
   <!--Our custom projects-->
  <project path="src/simple_navigation_goals" name="ros-autom/simple_navigation_goals" />
  
   <include name="snippets/upstream.xml" />

</manifest>
